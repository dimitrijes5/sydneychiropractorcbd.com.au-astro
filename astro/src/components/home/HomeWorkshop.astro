---
import PrimaryButton from "../PrimaryButton.astro";
import UnderlinedHeader from "../UnderlinedHeader.astro";
import Image from "astro/components/Image.astro";
import hcf from "../../../public/hcf.webp";
---
<div class="w-full py-10" style="background-color:#f2f4f6">
    <div class="mb-20 mx-auto flex flex-col justify-center items-center bg-white w-1/2 border border-gray-300 p-10" >
      <h1 class="text-2xl font-bold">Book A Clinic 27</h1>
      <h2 class="text-4xl mb-8 mt-4">Workshop Online</h2>
      <PrimaryButton variant="primary">Register</PrimaryButton>
    </div>
    <UnderlinedHeader>Health Insurance Accepted</UnderlinedHeader>
    <div class="mx-auto flex flex-row justify-center items-center w-1/2 gap-10 mt-10">
      <Image src={hcf} alt="HCF" class="w-1/4" loading="lazy" />
      <Image src={hcf} alt="HCF" class="w-1/4" loading="lazy" />
      <Image src={hcf} alt="HCF" class="w-1/4" loading="lazy" />
      <Image src={hcf} alt="HCF" class="w-1/4" loading="lazy" />

    </div>
  </div>